{% schema %}
{
  "name": "Unique Faqs",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "page",
      "id": "page",
      "label": "Select page of faqs for the crossponding products "
    },
    {
      "type": "header",
      "content": "All padding"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Top Padding",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Down Padding",
      "default": 36
    }
  ],
  "presets": [
    {
      "name": "Unique Faqs"
    }
  ]
}
{% endschema %}